<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Advanced tests suite">
    <listeners>
        <listener class-name="com.epam.reportportal.testng.ReportPortalTestNGListener" />
    </listeners>
    <test name="Advanced" parallel="classes" thread-count="1">
        <groups>
            <run>
                <include name="advanced"/>
            </run>
        </groups>
        <packages>
            <package name="com.epam.tests.*"/>
        </packages>
    </test>
</suite>
